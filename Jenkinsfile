pipeline {
    agent any   // run on any available Jenkins agent

    stages {
        stage('Build') {
            steps {
                echo "Building the project..."
            }
        }
        stage('Test') {
            steps {
                echo "Running tests..."
            }
        }
        stage('Deploy') {
            steps {
                echo "Deploying the project..."
            }
        }

        stage('Delivery') {
            steps {
                echo "Delivering the project..."
            }
        }
    }
}

